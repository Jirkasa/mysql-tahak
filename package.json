{
  "name": "mysql-tahak",
  "version": "1.0.0",
  "description": "MySQL Cheat Sheet website",
  "main": "index.js",
  "scripts": {
    "compile:less": "lessc ./less/main.less ./media/css/style.css",
    "nodemon:compile:less": "nodemon -e less -x \"npm run compile:less\"",
    "concat:css": "concat -o media/css/style.concat.css media/css/icofont.min.css media/css/style.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' media/css/style.concat.css -o media/css/style.prefix.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jirkasa/mysql-tahak.git"
  },
  "author": "Jiří Satora",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jirkasa/mysql-tahak/issues"
  },
  "homepage": "https://github.com/Jirkasa/mysql-tahak#readme",
  "dependencies": {
    "autoprefixer": "^10.3.1",
    "concat": "^1.0.3",
    "less": "^4.1.1",
    "nodemon": "^2.0.12",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1"
  }
}
